#set("content") {
  #if(count(users) > 0) {
    <table class="table table-bordered table-hover">
      <thead class="thead-light">
        <tr>
          <th>Name</th>
          <th>Username</th>
        </tr>
      </thead>
      <tbody>
        #for(user in users) {
          <tr>
            <td>#(user.name)</td>
            <td>#(user.username)</td>
          </tr>
        }
      </tbody>
    </table>
  } else {
    <h2>There are no users yet</h2>
  }
}
#embed("base")
